git ini            -- Inicializar repositorio git
git status         -- Devuelve ramas y ficheros
git add.           -- Añade ficheros para su seguimiento
git commit -m ""   -- Añade ficheros para su seguimiento
git log            -- Commit hechos
git branch         -- Devuelve las ramas del repositorio
git remote add origin https://github.com/victortoledo10/*** -- Añade repositorio remoto
git branch -M main -- Renombra la rama con el nombre 
git push -u origin main -- Sube el contenido al repositorio remoto
git clone 
git diff   -- diferencias






git branch nueva_rama    -- Crear rama nueva
git checkout nueva_rama  -- Posicionarte rama nueva
git push --set-upstream origin rama1 -- Enviar los cambios de la rama al repositorio remoto
git branch  -- Ves todas las ramas y en la que estas posicionado
git branch -d rama2 -- borra rama
git push origin --delete feature -- borrar rama en remoto


git pull

git checkout master
git merge feature









